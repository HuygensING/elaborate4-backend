<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d [%thread] %-5level %logger{48} - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="elaborate" level="warn" />

	<root level="warn">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>