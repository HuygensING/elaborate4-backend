<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!-- Configuration file for UrlRewriteFilter http://www.tuckey.org/urlrewrite/ -->
<urlrewrite>

  <rule>
    <note>
      The rule means that requests to /page/* will be redirected to / in a transparent fashion,
      that is, the users will see these URLs but in reality be served index.html.
    </note>
    <from>/page/.*</from>
    <to>/index.html</to>
  </rule>

  <rule>
    <from>/tekst/.*</from>
    <to>/index.html</to>
  </rule>

  <rule>
    <from>/entry/.*</from>
    <to>/index.html</to>
  </rule>

  <rule>
    <from>/zoeken/.*</from>
    <to>/index.html</to>
  </rule>

  <rule>
    <from>/search/.*</from>
    <to>/index.html</to>
  </rule>

  <rule>
    <from>/zoeken</from>
    <to>/index.html</to>
  </rule>

</urlrewrite>

